          <% project.images.forEach(function(image) { %>
            <div role="listitem" class="project-image-item y-dyn-item y-dyn-repeater-item">
              <img src="<%= image %>" loading="lazy" alt="" sizes="(max-width: 479px) 95vw, (max-width: 767px) 97vw, 95vw" class="project-detail-image">
            </div>
          <% }); %>
